{
    "name": "momik/laravel-google-sheet-i18n",
    "description": "Translate Laravel language files using Google Sheets",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Momik Shrestha",
            "email": "momik.shrestha@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "google/apiclient": "^2.12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelGoogleSheetI18n\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelGoogleSheetI18n\\GoogleSheetI18nServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}